  },
  "dependencies": {
    "@tanstack/react-router": "^1.125.6",
    "@zeronsh/router": "*",
    "ai": "^5.0.0-beta.9",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.125.6",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
