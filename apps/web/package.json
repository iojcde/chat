{
  "name": "@zeronsh/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun --port 2000 --hot src/index.ts",
